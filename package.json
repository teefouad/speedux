{
  "name": "speedux",
  "version": "3.0.0",
  "description": "An opinionated state management library based on Redux.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "babel src --out-dir lib",
    "build:umd": "cross-env FILE_NAME=speedux webpack",
    "build:umd:min": "cross-env NODE_ENV=production npm run build:umd",
    "clean": "rimraf lib dist",
    "lint": "eslint src --ext .js,.jsx",
    "test": "jest tests",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teefouad/speedux.git"
  },
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "redux",
    "action",
    "reducer",
    "generator",
    "javascript",
    "nodejs"
  ],
  "author": "Mostafa <tee.fouad@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teefouad/speedux/issues"
  },
  "homepage": "https://github.com/teefouad/speedux#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.4.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3"
  }
}
